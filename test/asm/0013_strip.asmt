## CODE

func foo(a)
{
  if (a is String)
  {
    print("got string");
  }
  if (a is Fixnum)
  {
    print("got fixnum");
  }
}

func bar()
{
  foo("str");
  foo(12);
}

## OPTIONS

--opt --strip --entry=bar

